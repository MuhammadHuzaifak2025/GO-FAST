{
  "expo": {
    "permissions": [
      "CAMERA"
    ],
    "name": "Gofast",
    "slug": "Gofast",
    "version": "1.0.0",
    "newArchEnablied": true,
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Gofast"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Gofast"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],[
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoibXVoYW1tYWRodXphaWZhNDMzNyIsImEiOiJjbTN4d2x2MmQxaXM0MnJxenlkMjg5OW9hIn0.LmpgQtqRQ5pWQLBk20tx3w",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
